@model IEnumerable<SchoolChoicePlayground.Models.School>
@{
    ViewBag.Title = "Home";
}

<h2>All Charters</h2>
<div ng-controller="TestController">{{test}}</div>

@foreach (var item in Model)
{
    <div class="row">
        <div class="col-lg-12">
            <div class="school">
                @item.name
            </div>
        </div>
    </div>
}

